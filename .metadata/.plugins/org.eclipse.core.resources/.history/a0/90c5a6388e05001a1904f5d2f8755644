public class Graph {
	
	int numberOfVertex;
	List<Integer> adjList;
	
	//constructor
	public Graph(int numberOfVertex) {
		this.numberOfVertex = numberOfVertex;
		
		//List<Integer> adjList = new ArrayList<numberOfVertex>();
		List<Integer> adjList = new ArrayList<Integer>(numberOfVertex);
		
		for(int i =0; i<numberOfVertex; i++) {
			adjList.se = new LinkedList<>();
		}
	}
}
